{
    "name": "Ava-Ire",
    "version": "0.5.8",
    "description": "A Discord Bot",
    "main": "start.js",
    "maintainers": [
        {
            "name": "Alexis Tan",
            "email": "alexis@sen-dev.com"
        }
    ],
    "author": {
        "name": "Alexis Tan",
        "email": "alexis@sen-dev.com",
        "url": "https://senither.com"
    },
    "scripts": {
        "start": "node start.js",
        "test": "xo && mocha tests",
        "generate-docs": "esdoc"
    },
    "engines": {
        "node": "4.x.x - 6.x.x",
        "npm": ">=3.x.x"
    },
    "xo": {
        "space": 4,
        "semicolon": true,
        "globals": [
            "app",
            "bot"
        ],
        "rules": {
            "camelcase": 0,
            "new-parens": 0,
            "guard-for-in": 0,
            "no-eval": 0,
            "no-unused-vars": 0,
            "no-prototype-builtins": 0,
            "unicorn/filename-case": 0,
            "import/no-dynamic-require": 0
        }
    },
    "license": "MIT",
    "dependencies": {
        "apiai": "4.0.1",
        "discordie": "^0.11.0",
        "dot-object": "^1.5.4",
        "fs-extra": "^2.0.0",
        "knex": "^0.12.7",
        "lodash": "^4.17.4",
        "mysql": "^2.13.0",
        "node-schedule": "^1.2.0",
        "request": "^2.81.0",
        "require-directory": "^2.1.1",
        "semver-diff": "^2.1.0",
        "winston": "^2.3.1",
        "winston-daily-rotate-file": "^1.4.6",
        "youtube-api": "^2.0.6",
        "youtube-dl": "^1.11.1",
        "ytdl-core": "^0.10.0"
    },
    "devDependencies": {
        "minami": "^1.1.1",
        "mocha": "^3.2.0",
        "xo": "^0.17.1"
    },
    "snyk": true
}
